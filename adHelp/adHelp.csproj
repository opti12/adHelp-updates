  <!-- 빌드 후 자동 디지털 서명 -->
  <Target Name="SignExecutable" AfterTargets="Build" Condition="Exists('$(ProjectDir)kkh_helpdesk_cert.pfx')">
    <PropertyGroup>
      <SignToolPath>C:\Program Files (x86)\Microsoft SDKs\ClickOnce\SignTool\signtool.exe</SignToolPath>
      <CertificatePath>$(ProjectDir)kkh_helpdesk_cert.pfx</CertificatePath>
      <CertificatePassword>Helpdesk2693</CertificatePassword>
      <OutputExecutable>$(OutputPath)$(AssemblyName).exe</OutputExecutable>
    </PropertyGroup>
    <Message Text="디지털 서명 적용 중: $(OutputExecutable)" Importance="high" />
    <Exec Command="&quot;$(SignToolPath)&quot; sign /f &quot;$(CertificatePath)&quot; /p &quot;$(CertificatePassword)&quot; /tr http://timestamp.digicert.com /td sha256 /fd sha256 &quot;$(OutputExecutable)&quot;" Condition="Exists('$(SignToolPath)') and Exists('$(OutputExecutable)')" ContinueOnError="false" />
    <Message Text="디지털 서명이 성공적으로 적용되었습니다." Importance="high" />
  </Target>
  <!-- 서명 검증 -->
  <Target Name="VerifySignature" AfterTargets="SignExecutable" Condition="Exists('$(ProjectDir)kkh_helpdesk_cert.pfx')">
    <PropertyGroup>
      <SignToolPath>C:\Program Files (x86)\Microsoft SDKs\ClickOnce\SignTool\signtool.exe</SignToolPath>
      <OutputExecutable>$(OutputPath)$(AssemblyName).exe</OutputExecutable>
    </PropertyGroup>
    <Message Text="서명 검증 중: $(OutputExecutable)" Importance="high" />
    <Exec Command="&quot;$(SignToolPath)&quot; verify /pa &quot;$(OutputExecutable)&quot;" Condition="Exists('$(SignToolPath)') and Exists('$(OutputExecutable)')" ContinueOnError="true" />
  </Target>
